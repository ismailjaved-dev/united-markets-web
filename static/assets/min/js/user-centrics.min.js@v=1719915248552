const gclidParam="gclid",COOKIE_EXPIRATION=2592e3,UC_CONSTANTS={eventTriggered:"UC_UI_CMP_EVENT",marketingSlug:"marketing",gaConsent:"google analytics",events:{ACCEPT_ALL:"ACCEPT_ALL",SAVE:"SAVE"}},ANALYTICS_COOKIES={originalGa:"_ga",ga:"analytics_ga",gclid:"analytics_gclid"};function setUCListener(){window.addEventListener(UC_CONSTANTS.eventTriggered,e=>{const t=e.detail.type.toUpperCase();(t===UC_CONSTANTS.events.ACCEPT_ALL||t===UC_CONSTANTS.events.SAVE&&isGAConsentAccepted())&&storeAnalyticsInfo()})}function storeAnalyticsInfo(){queryParams[gclidParam]&&setCookie(ANALYTICS_COOKIES.gclid,queryParams[gclidParam],COOKIE_EXPIRATION);const e=getCookie(ANALYTICS_COOKIES.originalGa);e&&setCookie(ANALYTICS_COOKIES.ga,e,COOKIE_EXPIRATION)}function isGAConsentAccepted(){return(window.UC_UI&&window.UC_UI.getServicesBaseInfo()||[]).some(({categorySlug:e,consent:t,name:n})=>e&&e.toLowerCase()===UC_CONSTANTS.marketingSlug&&n.toLowerCase()===UC_CONSTANTS.gaConsent&&!!t&&!!t.status)}setUCListener(),setTimeout(()=>{isGAConsentAccepted()&&storeAnalyticsInfo()},500);